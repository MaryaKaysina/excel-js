{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "require-jsdoc": "off",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "operator-linebreak": [
      "error",
      "before"
    ]
  },
  "plugins": [
    "jest"
  ],
  "extends": [
    "eslint:recommended",
    "google"
  ]
}
